 #include<iostream>
 using namespace std;
 int main()
 {
     int a[2][2]={{1,2},{3,4}};
     int key;
     cin>>key;
     int flag=0;
     for(int i=0; i<2; i++)
     {
         for(int j=0; j<2; j++)
         {
             if(a[i][j]==key)
             {
                 cout<<key<<endl;
                 break;
             }else{
                 flag++;
             }
         }
     }
     if(flag==4)
     {
         cout<<"NOt found"<<endl;
     }
 }
